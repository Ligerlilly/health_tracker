<tr>
<td>
<%= form_for Food.new, remote: true do |f| %>
     <%= f.label :name, 'Add a Food' %>
     <div class="form-group">
       <%= f.text_field :name, value: params[:search] %>
       <%= f.text_field :calories, placeholder: 'Enter calories' %>

     </div>
  <%= f.submit %>
<% end %>
</td>
</tr>
